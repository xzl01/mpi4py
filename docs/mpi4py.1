.SS Examples
.sp
\fBtest\-pkl5\-1.py\fP
.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
import numpy as np
from mpi4py import MPI
from mpi4py.util import pkl5

comm = pkl5.Intracomm(MPI.COMM_WORLD)  # comm wrapper
size = comm.Get_size()
rank = comm.Get_rank()
dst = (rank + 1) % size
src = (rank \- 1) % size

sobj = np.full(1024**3, rank, dtype=\(aqi4\(aq)  # > 4 GiB
sreq = comm.isend(sobj, dst, tag=42)
robj = comm.recv (None, src, tag=42)
sreq.Free()

assert np.min(robj) == src
assert np.max(robj) == src
.ft P
.fi
.UNINDENT
.UNINDENT
.sp
\fBtest\-pkl5\-2.py\fP
.INDENT 0.0
.INDENT 3.5
.sp
.nf
.ft C
import numpy as np
from mpi4py import MPI
from mpi4py.util import pkl5

comm = pkl5.Intracomm(MPI.COMM_WORLD)  # comm wrapper
size = comm.Get_size()
rank = comm.Get_rank()
dst = (rank + 1) % size
src = (rank \- 1) % size

sobj = np.full(1024**3, rank, dtype=\(aqi4\(aq)  # > 4 GiB
sreq = comm.isend(sobj, dst, tag=42)

status = MPI.Status()
rmsg = comm.mprobe(status=status)
assert status.Get_source() == src
assert status.Get_tag() == 42
rreq = rmsg.irecv()
robj = rreq.wait()

sreq.Free()
assert np.max(robj) == src
assert np.min(robj) == src
.ft P
.fi
